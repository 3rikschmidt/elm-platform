{
  "name": "elm",
  "version": "0.15.1-alpha",
  "description": "The Elm Platform: Binaries for the Elm programming language.",
  "preferGlobal": true,
  "license": "BSD-3-Clause",
  "repository": {
    "type": "git",
    "url": "https://github.com/elm-lang/elm-platform"
  },
  "bugs": "https://github.com/elm-lang/elm-platform/issues",
  "author": {
    "name": "Richard Feldman",
    "email": "richard.t.feldman@gmail.com",
    "url": "https://github.com/rtfeldman"
  },
  "engines": {
    "node": ">=0.10.0"
  },
  "main": "./index.js",
  "scripts": {
    "install": "node-pre-gyp install --fallback-to-build",
    "prepublish": "node prepublish.js",
    "build-from-source": "echo 'foo'"
  },
  "files": [
    "binding.gyp",
    "BuildFromSource.hs",
    "binwrap.js",
    "binwrappers"
  ],
  "keywords": [
    "bin",
    "binary",
    "binaries",
    "elm",
    "elm-make",
    "elm-package",
    "elm-platform",
    "elm-reactor",
    "elm-repl",
    "platform"
  ],
  "bin": {
    "elm": "binwrappers/elm",
    "elm-make": "binwrappers/elm-make",
    "elm-package": "binwrappers/elm-package",
    "elm-reactor": "binwrappers/elm-reactor",
    "elm-repl": "binwrappers/elm-repl"
  },
  "binary": {
    "module_name": "elm",
    "module_path": "./Elm-Platform/{major}.{minor}.{patch}/",
    "host": "https://dl.bintray.com",
    "remote_path": "./elmlang/elm-platform/{major}.{minor}.{patch}/{configuration}/",
    "package_name": "{platform}-{arch}.tar.gz"
  },
  "bundledDependencies": [
    "node-pre-gyp"
  ],
  "dependencies": {
    "node-pre-gyp": "0.6.13"
  }
}
